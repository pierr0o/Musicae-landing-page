<!-- banner -->
<div class="container-fluid" id="banner">
	<div class="container text-center">
		<div class="row">
			<div class="col-xs-8 col-xs-offset-2">
				<h1>La musique par ceux qui la jouent</h1>
				<br>
				<br>
				<%= link_to( "<strong>Ecoutez !<strong>", "#music" , class:'btn-rounded') %>
			</div>
		</div>
	</div>
</div>
 


<!-- Demo -->

